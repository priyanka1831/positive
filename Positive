public class Positive {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int  number=544;
     //write a loop
     if(number>0)
     {
    	 System.out.println(number+" is positive");
     }
     if(number<0)
     {
    	 System.out.println(number+" is negative");
     }
    

	}
}
